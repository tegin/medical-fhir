<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="assets_backend"
        name="assets_backend"
        inherit_id="web.assets_backend"
    > <!-- backend (herencia de erp) o frontend (website)-->
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/owl_tutorial_views/static/src/components/impression_component.js"
            />

            <script
                type="text/javascript"
                src="/owl_tutorial_views/static/src/owl_tree_view/owl_tree_view.js"
            />
            <script
                type="text/javascript"
                src="/owl_tutorial_views/static/src/owl_tree_view/owl_tree_model.js"
            />
            <script
                type="text/javascript"
                src="/owl_tutorial_views/static/src/owl_tree_view/owl_tree_controller.js"
            />
            <script
                type="text/javascript"
                src="/owl_tutorial_views/static/src/owl_tree_view/owl_tree_renderer.js"
            />
        </xpath>
        <xpath expr="link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/owl_tutorial_views/static/src/components/tree_item/tree_item.scss"
            />
            <link
                rel="stylesheet"
                type="text/scss"
                href="/owl_tutorial_views/static/src/owl_tree_view/owl_tree_view.scss"
            />
        </xpath>
    </template>
</odoo>
